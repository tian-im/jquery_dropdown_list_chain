# A sample Guardfile
# More info at https://github.com/guard/guard#readme

guard 'coffeescript', :input => 'src', :output => 'src'

guard 'coffeescript', :input => 'spec', :output => 'spec'

guard 'coffeescript', :input => 'examples', :output => 'examples'

guard 'haml' do
  watch /^.+\.html\.haml/
end

guard 'compass'